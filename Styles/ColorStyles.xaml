<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="1,2" Opacity="0.65">
        <GradientStop Color="#FFA95D09" Offset="0.0"/>
        <GradientStop Color="#FF3904A9" Offset="1.0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="cs_BorderBrush" Color="#FF3904A9"/>

    <SolidColorBrush x:Key="cs_BBackground" Color="#FFEDDCEB" Opacity="0.5"/>

    <SolidColorBrush x:Key="cs_Text" Color="#FFF5D9E3"/>

    <SolidColorBrush x:Key="cs_TxtBBrush" Color="#FF422A02"/>

</ResourceDictionary>